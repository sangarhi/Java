<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>DireccionesController</servlet-name>
		<servlet-class>com.front.pDireccion.controller.DireccionesController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DireccionController</servlet-name>
		<servlet-class>com.front.pDireccion.controller.DireccionController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DireccionesController</servlet-name>
		<url-pattern>/direcciones</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DireccionController</servlet-name>
		<url-pattern>/direccion</url-pattern>
	</servlet-mapping>
</web-app>